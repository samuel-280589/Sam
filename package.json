    "underscore": "^1.8.3",
    "url": "^0.11.0",
    "uuid": "^3.0.1",
    "vso-node-api": "^5.1.1",
    "vsts-device-flow-auth": "^1.136.0",
    "winston": "2.3.1",
    "xml2js": "^0.4.17",
    "xmldoc": "^0.5.1"
  }
}
